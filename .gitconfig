[alias]
    co = checkout
    ci = commit
    st = status
    br = branch
    hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
    type = cat-file -t
    dump = cat-file -p
    gs = git status 
    ga = git add 
    gb = git branch 
    gc = git commit
    gd = git diff
    gco = git checkout 
    gk = gitk --all&
    gx = gitx --all